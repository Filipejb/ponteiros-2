#include <stdio.h>

void imprimirArray(int *array, int tamanho) {
    int *ptr;

    for (ptr = array; ptr < array + tamanho; ptr++) {
        printf("%d ", *ptr);
    }
    printf("\n");
}

int main() {
    int array[] = {1, 2, 3, 4};
    int tamanho = sizeof(array) / sizeof(array[0]);

    imprimirArray(array, tamanho);

    return 0;
}
